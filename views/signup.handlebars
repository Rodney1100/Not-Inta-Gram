  <div class="overlay-container col s12 m6">
    <form class="signup-form">
      <h4>Create Account</h4>
      <input type="text" id="username-signup" placeholder="Username" />
      <input type="email" id="email-signup" placeholder="Email" />
      <input type="password" id="password-signup" placeholder="Password" />
      <button class="button1" type="submit" id="signIn">Sign up</button>
    </form>
    <div class="center">
      <h4>Or log in here</h4>
      <p>To keep connected with friends, login with your personal info</p>
      <a href="/login">
        <button class="button1" id="signIn"> Log In</button>
      </a>
    </div>
  </div>

  <div class="form-container col s12 m6 sign-up-container">
    <div class="overlay">
      <div class="overlay-panel overlay-right">
        <h4>Or log in here</h4>
        <p>To keep connected with friends, login with your personal info</p>
        <a href="/login">
          <button class="button1" id="signIn"> Log In</button>
        </a>
      </div>
    </div>
  </div>
</div>

